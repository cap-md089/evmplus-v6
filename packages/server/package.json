{
	"name": "evmplus-org-server",
	"version": "6.26.0",
	"private": true,
	"scripts": {
		"start": "node --no-warnings dist/index.js",
		"test": "jest",
		"build": "ttsc --build tsconfig.build.json",
		"watch": "ttsc --build tsconfig.build.json --watch",
		"check": "ttsc --build tsconfig.build.json --noEmit",
		"clean": "prettier --write 'src/**/*.ts'",
		"clean-watch": "onchange '**/*.ts' -- prettier --write {{changed}}",
		"lint": "eslint src/**/*.ts src/*.ts",
		"debug": "nodemon --no-warnings --inspect=0.0.0.0:5858 --watch ../discord-bot --watch ../server-common --watch ../common-lib --watch ./ ./dist/index.js",
		"debug:brk": "nodemon --no-warnings --inspect-brk=0.0.0.0:5858 --watch ../discord-bot --watch ../server-common --watch ../common-lib --watch ./ ./dist/index.js",
		"docker-debug": "cd ../.. && docker-compose -f docker-compose.dev.yml up main"
	},
	"license": "GPL-2.0",
	"main": "dist/index.js",
	"dependencies": {
		"@mysql/xdevapi": "8.0.19",
		"auto-client-api": "^1.3.22",
		"body-parser": "^1.18.2",
		"busboy": "^0.3.1",
		"cheerio": "^1.0.0-rc.2",
		"common-lib": "^1.27.0",
		"cors": "^2.8.5",
		"csv-parse": "^4.8.5",
		"debug": "^4.3.1",
		"discord-bot": "^1.18.0",
		"dotenv": "^8.2.0",
		"exif": "^0.6.0",
		"express": "^4.16.3",
		"googleapis": "^47.0.0",
		"luxon": "^1.3.1",
		"markdown": "^0.5.0",
		"mime-types": "^2.1.19",
		"morgan": "^1.9.1",
		"onchange": "^6.1.0",
		"pdfkit": "^0.10.0",
		"promise-mysql": "^3.2.1",
		"request": "^2.87.0",
		"request-promise": "^4.2.2",
		"serve-favicon": "^2.4.5",
		"server-common": "^1.24.0",
		"speakeasy": "^2.0.0",
		"ssh2-sftp-client": "^5.2.1",
		"uuid": "^3.2.1"
	},
	"devDependencies": {
		"@types/body-parser": "^1.16.8",
		"@types/busboy": "^0.2.3",
		"@types/cheerio": "^0.22.7",
		"@types/cookie-parser": "^1.4.1",
		"@types/cors": "^2.8.6",
		"@types/csv-parse": "^1.1.11",
		"@types/debug": "^4.1.5",
		"@types/error-stack-parser": "^2.0.0",
		"@types/es6-promise": "^3.3.0",
		"@types/express": "^4.16.0",
		"@types/gulp-tslint": "^4.2.0",
		"@types/jest": "^26",
		"@types/luxon": "^1.2.1",
		"@types/mime-types": "^2.1.0",
		"@types/morgan": "^1.7.35",
		"@types/mysql": "^2.15.9",
		"@types/node": "^13.13.4",
		"@types/react": "^16.8.4",
		"@types/request-promise": "^4.1.44",
		"@types/request-promise-native": "^1.0.15",
		"@types/speakeasy": "^2.0.5",
		"@types/supertest": "^2.0.5",
		"@types/uuid": "^3.4.3",
		"@typescript-eslint/eslint-plugin": "^4.25.0",
		"@typescript-eslint/parser": "^4.25.0",
		"eslint": "^7.27.0",
		"eslint-config-prettier": "^8.3.0",
		"express-unit": "^2.1.1",
		"gulp": "^4",
		"gulp-shell": "^0.7.0",
		"jest": "^26",
		"jest-cli": "^26",
		"jest-watch-typeahead": "^0.6.1",
		"mysql": "^2.18.1",
		"nodemon": "^1.19.4",
		"prettier": "^2.2.1",
		"server-jest-config": "^1.0.7",
		"supertest": "^3.1.0",
		"ts-jest": "^26",
		"tslint": "^5.19.0",
		"tslint-config-prettier": "^1.13.0",
		"ttypescript": "^1.5.10",
		"typescript": "^4.2.3"
	},
	"jest": {
		"globals": {
			"ts-jest": {
				"tsConfig": "tsconfig.test.json",
				"diagnostics": false
			}
		},
		"testRegex": "(/src/__tests__/.*\\.ts)",
		"testPathIgnorePatterns": [
			"/node_modules/",
			"<rootDir>/src/__tests__/consts.ts",
			"<rootDir>/src/__tests__/TestUtils.ts",
			"<rootDir>/src/__tests__/EitherMatcher.ts"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"moduleFileExtensions": [
			"ts",
			"js",
			"json",
			"node"
		]
	}
}
