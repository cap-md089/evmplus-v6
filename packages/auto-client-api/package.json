{
	"name": "auto-client-api",
	"version": "1.3.23",
	"description": "Automatically generates APIs for the TypeScript Event Manager server for use on a JavaScript client",
	"scripts": {
		"build": "tsc --build tsconfig.build.json",
		"watch": "tsc --build tsconfig.build.json --watch",
		"check": "tsc --build tsconfig.build.json --noEmit",
		"lint": "eslint src/**/*.ts src/*.ts index.d.ts"
	},
	"author": "Andrew Rioux",
	"license": "GPL-2.0",
	"private": true,
	"dependencies": {
		"debug": "^4.3.1",
		"typescript": "^4.2.3"
	},
	"devDependencies": {
		"@types/debug": "^4.1.5",
		"@types/node": "^13.13.4",
		"@typescript-eslint/eslint-plugin": "^4.25.0",
		"@typescript-eslint/parser": "^4.25.0",
		"common-lib": "^1.28.0",
		"eslint": "^7.27.0",
		"eslint-config-prettier": "^8.3.0",
		"prettier": "^2.2.1",
		"tslint": "^6.1.2",
		"tslint-config-prettier": "^1.18.0"
	}
}
