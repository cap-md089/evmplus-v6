		"server-common": "^1.25.0",
		"server-jest-config": "^1.1.0"
	},
	"devDependencies": {
		"@types/debug": "^4.1.5",
		"@typescript-eslint/eslint-plugin": "^4.25.0",
		"@typescript-eslint/parser": "^4.25.0",
		"eslint": "^7.27.0",
		"eslint-config-prettier": "^8.3.0",
		"jest-watch-typeahead": "^0.6.1",
