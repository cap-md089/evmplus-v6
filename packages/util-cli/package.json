{
	"name": "util-cli",
	"version": "1.14.0",
	"description": "CLI utilities for server admins of the CAPUnit suite",
	"private": true,
	"scripts": {
		"build": "ttsc --build tsconfig.build.json && chmod +x dist/*.js",
		"watch": "ttsc --build tsconfig.build.json --build",
